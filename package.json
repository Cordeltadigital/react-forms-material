{
  "name": "react-material-functional-forms",
  "version": "0.0.1",
  "description": "Beautiful, ultra simple, stateless, validated forms for use in React function components",
  "main": "dist.js",
  "module": "index.js",
  "scripts": {
    "test": "jest",
    "build": "babel index.js --out-file dist.js",
    "prepublish": "babel index.js --out-file dist.js --source-maps false"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.2",
    "react-functional-forms": "^0.0.1"
  },
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "babel-jest": "^24.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "jest": "^24.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/test/setup/styleMock.js"
